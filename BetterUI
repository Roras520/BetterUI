	return {
		Accent = Color3.fromRGB(86, 195, 245),
		ElementBorder = Color3.fromRGB(25, 25, 25),
		TextColor = Color3.fromRGB(230, 230, 230),
		SubTextColor = Color3.fromRGB(160, 160, 160),
