local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")
local TextService = game:GetService("TextService")

local LocalPlayer = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui") or LocalPlayer.PlayerGui

local SYLIX_LOADED = _G.SYLIX_LOADED

if SYLIX_LOADED then
	for _, gui in pairs(CoreGui:GetChildren()) do
		if gui.Name == "SylixUI" or gui.Name == "SylixUILoading" or gui.Name == "Notifications" then
			gui:Destroy()
		end
	end
	if _G.Sylix and _G.Sylix.ScreenGui and _G.Sylix.ScreenGui.Parent then
		_G.Sylix.ScreenGui:Destroy()
	end
	_G.Sylix = nil
	_G.SYLIX_LOADED = false
end

if not game:IsLoaded() then
	game.Loaded:Wait()
end

_G.SYLIX_LOADED = true

local Sylix = {}
_G.Sylix = Sylix

local ReplicatedStorage = game:GetService("ReplicatedStorage")

local SylixModulesFolder = Instance.new("Folder")
SylixModulesFolder.Name = "SylixModules"
SylixModulesFolder.Parent = ReplicatedStorage

function createModule(name, src)
	local module = Instance.new("ModuleScript")
	module.Name = name
	module.Parent = SylixModulesFolder
	module.Source = src
	return module
end

local moduleCache = {}
local SafeLoadIteration = 0
local function SafeLoad(sources)
	for _, source in ipairs(sources or {}) do
        SafeLoadIteration = SafeLoadIteration + 1
		local ok, result
		if typeof(source) == "string" then
			if moduleCache[source] ~= nil then
				return moduleCache[source]
			end
			ok, result = pcall(function()
				local code = game:HttpGet(source, true)
				if not code or code == "" then
					error("Empty response from server")
				end
				local loaded = loadstring(code)
				if not loaded then
					error("Failed to compile code")
				end
				return loaded()
			end)
			if ok and result then
				moduleCache[source] = result
				return result
			else
				warn("SafeLoad failed for:", source, "- Error:", tostring(result))
			end
		elseif typeof(source) == "function" then
			ok, result = pcall(source)
			if ok and result then print("Failed to retrive code from github, using hardcoded module source. Module Loader Iteration: " .. tostring(SafeLoadIteration)) return result end
		end
	end
	return nil
end

Sylix.SafeLoad = SafeLoad

