--[[
    Sylix UI Library
    Created by Stacm/Stack
    Build 1.6.1
    Date: 2025-11-10
    Changelogs:
	- Accent bar is now completely invisible on UI initialization
	- Removed all font editor options and functionality
	- Added real-time layout switching (Vertical/Horizontal)
	- Added ChangeLayout function for dynamic layout changes
	- Added layout changer option to Settings tab
	- Separator line now hides in horizontal layout
	- Tab sidebar is now scrollable in both layouts
	- Accent bars auto-disable in horizontal layout
]]

local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")
local TextService = game:GetService("TextService")

local LocalPlayer = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui") or LocalPlayer.PlayerGui

local SYLIX_LOADED = _G.SYLIX_LOADED

if SYLIX_LOADED then
	for _, gui in pairs(CoreGui:GetChildren()) do
		if gui.Name == "SylixUI" or gui.Name == "SylixUILoading" or gui.Name == "Notifications" then
			gui:Destroy()
		end
	end
	if _G.Sylix and _G.Sylix.ScreenGui and _G.Sylix.ScreenGui.Parent then
		_G.Sylix.ScreenGui:Destroy()
	end
	_G.Sylix = nil
	_G.SYLIX_LOADED = false
end

if not game:IsLoaded() then
	game.Loaded:Wait()
end

_G.SYLIX_LOADED = true

local Sylix = {}
_G.Sylix = Sylix

local UtilitiesLoader = loadstring(game:HttpGet("https://raw.githubusercontent.com/Roras520/Lib/refs/heads/main/UtilLoader"))()

local MODULES = {
	Acrylic = UtilitiesLoader.LoadModule("Acrylic"),
	Notifications = UtilitiesLoader.LoadModule("Notifications"),
}

